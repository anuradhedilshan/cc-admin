    "@vendure/core": "3.4.3",
    "@vendure/dashboard": "^3.4.3",
    "@vendure/email-plugin": "3.4.3",
    "@vendure/graphiql-plugin": "3.4.3",
    "better-sqlite3": "12.4.1",
    "dotenv": "17.2.3",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "6.0.0",
    "@graphql-codegen/typescript": "5.0.1",
    "@vendure/cli": "3.4.3",
    "concurrently": "9.2.1",
    "typescript": "5.8.2"
  }
}
